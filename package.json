  "scripts": {
    "start": "node pm2.js",
    "restart": "pm2 restart zellwk.com --update-env",
    "server": "nodemon start.js --ignore 'src/**/*.js' --ignore 'dev/**/*' --ignore 'dist/**/*'",
    "dev": "NODE_ENV=development gulp",
    "eleventy": "rm -rf dist && eleventy --watch --config=eleventy.js --quiet",
    "eleventy-debug": "DEBUG=Eleventy* eleventy",
    "build": "NODE_ENV=production gulp build",
    "observe": "NODE_ENV=production npm run server & gulp serve",
    "deploy": "npm run build && gulp deploy",
    "deployCI": "gulp deployCI",
    "test": "jest --watch"
  },
  "standard": {
    "ignore": [
      "**/prism.js"
    ]
  },
